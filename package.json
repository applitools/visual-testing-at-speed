{
  "name": "redux-todomvc-example",
  "version": "0.0.0",
  "description": "Redux TodoMVC example",
  "scripts": {
    "start": "node server.js",
    "test": "npm-run-all -p -c 'test:*'",
    "test:components": "cross-env NODE_ENV=test mocha --recursive --compilers js:babel-register --require ./test/setup.js",
    "test:storybook": "eyes-storybook",
    "test:cypress": "npm-run-all -p -r start cypress-run",
    "cypress-run": "cypress run",
    "eyes-cypress": "npm-run-all -p -r start cypress-open",
    "eyes-selenium": "npm-run-all -p -r start mocha-selenium",
    "mocha-selenium": "mocha --no-timeouts --exit selenium/*.test.js",
    "cypress-open": "cypress open",
    "eyes-storybook": "eyes-storybook",
    "storybook": "start-storybook -p 9001"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reactjs/redux.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjs/redux/issues"
  },
  "homepage": "http://redux.js.org",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.1.2",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1",
    "stack-source-map": "^1.0.4"
  },
  "devDependencies": {
    "@applitools/eyes.cypress": "^3.3.22",
    "@applitools/eyes.selenium": "^1.9.0-alpha",
    "@applitools/eyes.storybook": "^2.3.4",
    "@kadira/storybook": "^2.24.0",
    "@kadira/storybook-addon-comments": "^1.1.1",
    "@kadira/storybook-database-cloud": "^2.2.0",
    "@kadira/storybook-deployer": "^1.0.0",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "babel-register": "^6.3.13",
    "chromedriver": "^2.45.0",
    "cross-env": "^1.0.7",
    "cypress": "^3.1.5",
    "expect": "^1.8.0",
    "express": "^4.13.3",
    "jsdom": "^5.6.1",
    "mocha": "^5.0.0",
    "node-libs-browser": "^0.5.2",
    "npm-run-all": "^4.1.5",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^15.0.1",
    "selenium-webdriver": "^3.6.0",
    "style-loader": "^0.12.3",
    "todomvc-app-css": "^2.0.1",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.9.1"
  }
}
